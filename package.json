{
    "name": "fuel-rats-v3",
    "packageManager": "yarn@3.2.0",
    "private": true,
    "scripts": {
        "[-] Deployment": "",
        "windows": "yarn build:core && altv-server.exe",
        "linux": "yarn build:core && ./start.sh",
        "dev": "nodemon --watch \"./src/**\" --ext \"ts\" --exec \"yarn build:dev && altv-server.exe\"",
        "[-] Build": "",
        "build:dev": "yarn clean && yarn build:client && yarn build:server && yarn copy:mods && yarn copy:configs",
        "build:core": "yarn clean && yarn build:client && yarn build:server && yarn build:webview && yarn copy:mods && yarn copy:configs",
        "[-] Module Builds": "",
        "build:client": "yarn workspace @fuelrats/client build",
        "build:server": "yarn workspace @fuelrats/server build",
        "build:webview": "yarn workspace @fuelrats/webview build",
        "[-] Copy Files": "",
        "copy:mods": "yarn workspace @fuelrats/mods copy",
        "copy:configs": "yarn workspace @fuelrats/configs copy",
        "[-] Development": "",
        "vue-dev": "yarn workspace @fuelrats/webview dev",
        "[-] Updates": "",
        "update": "altv-pkg d release",
        "[-] Utilities": "",
        "clean": "yarn workspace @fuelrats/utility clean"
    },
    "workspaces": [
        "src/*"
    ],
    "devDependencies": {
        "altv-pkg": "^1.2.0",
        "esbuild": "^0.14.34",
        "nodemon": "^2.0.15",
        "prettier": "^2.6.2",
        "typescript": "^4.6.3"
    },
    "type": "module"
}